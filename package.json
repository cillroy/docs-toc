{
	"name": "docs-toc",
	"displayName": "docs-toc",
	"description": "",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.docs-toc",
		"onCommand:extension.docs-toc-html"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.docs-toc",
				"title": "Table of Contents",
				"category": "Docs"
			}
		],
		"keybindings": [
			{
				"command": "extension.docs-toc",
				"key": "ctrl+d ctrl+t",
				"mac": "cmd+d cmd+t"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"license": "SEE LICENSE IN LICENSE",
	"repository": {
		"type": "git",
		"url": "https://github.com/cillroy/docs-toc.git"
	}
}
